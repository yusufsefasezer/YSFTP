    <BitmapImage x:Key="bitmapImageFile" UriSource="Default/file.png" />
    <BitmapImage x:Key="bitmapImageFolder" UriSource="Default/folder.png" />
    <BitmapImage x:Key="bitmapImageInformation" UriSource="Default/information.png" />
    <BitmapImage x:Key="bitmapImageRefresh" UriSource="Default/refresh.png" />
